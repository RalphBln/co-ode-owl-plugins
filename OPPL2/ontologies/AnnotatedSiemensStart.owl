END;
Content builder</patterns:contentComponentPattern>
        <patterns:situationPattern rdf:datatype="&xsd;string"
            >?situation:CLASS ,
?consideration:CLASS ,
?element:CLASS ,
?includeConsideration:CLASS = OWLObjectProperty_01235005942259929000 some ?consideration  ,
?includeElement:CLASS = OWLObjectProperty_01240253589440041000 some ?element  ,
?allConsiderations:CLASS = createIntersection(?includeConsideration . VALUES ),
?allElements:CLASS = createIntersection(?includeElement . VALUES ) BEGIN
ADD
$thisClass equivalentTo ?situation and ?allConsiderations and ?allElements
END;
Situations builder</patterns:situationPattern>
        <patterns:elementPattern
            >?element:CLASS,
?exposedSubElement:CLASS,
?hiddenSubElement:CLASS,
?exposedFeature:CLASS,
?hiddenFeature:CLASS,
?quantity:CLASS,
?hasExposedSubElement:CLASS = OWLObjectProperty_01241019394335806000 some ?exposedSubElement,
?hasHiddenSubElement:CLASS = OWLObjectProperty_01241019337607959000 some ?hiddenSubElement,
?hasExposedFeature:CLASS = OWLObjectProperty_01241018787395832000 some ?exposedFeature,
?hasHiddenFeature:CLASS = OWLObjectProperty_01241019324612330000 some ?hiddenFeature,
?hasQuantity:CLASS = OWLObjectProperty_01237305548139854000 some ?quantity,
?allExposedSubElements:CLASS = createIntersection(?hasExposedSubElement . VALUES ),
?allHiddenSubElements:CLASS = createIntersection(?hasHiddenSubElement . VALUES ),
?allExposedFeatures:CLASS = createIntersection(?hasExposedFeature . VALUES ),
?allHiddenFeatures:CLASS = createIntersection(?hasHiddenFeature . VALUES ),
?allQuantities:CLASS = createIntersection(?hasQuantity . VALUES )
BEGIN
ADD $thisClass equivalentTo ?element and ?allExposedSubElements and ?allHiddenSubElements and ?allExposedSubElements and ?allExposedFeatures and ?allHiddenFeatures and ?allQuantities
END;</patterns:elementPattern>
        <patterns:featurePattern
            >?feature:CLASS ,
?element:CLASS ,
?quantity:CLASS ,
?value:CLASS ,
?hasElement:CLASS = OWLObjectProperty_01240253589440041000 some ?element ,
?hasQuantity:CLASS = OWLObjectProperty_01237305548139854000 some ?quantity  ,
?hasValue:CLASS = OWLObjectProperty_01256814591556773000 some ?value  ,
?allElements:CLASS = createIntersection(?hasElement . VALUES )  ,
?allQuantities:CLASS = createIntersection(?hasQuantity . VALUES )  ,
?allValues:CLASS = createIntersection(?hasValue . VALUES )
BEGIN
ADD $thisClass equivalentTo ?feature and ?allElements and ?allQuantities and ?allValues
END;
Feature builder</patterns:featurePattern>
        <patterns:quantityPattern
            >?quantity:CLASS ,
?integerValue:CONSTANT ,
?floatValue:CONSTANT
BEGIN
ADD $thisClass equivalentTo ?quantity and OWLDataProperty_01256840047976240000 value ?integerValue and OWLDataProperty_01256840047976240000 value ?floatValue
END;
