<?xml version="1.0" ?>

<plugin>

    <extension id="AnnotateView"
               point="org.protege.editor.core.application.ViewComponent">
        <label value="Annotate View"/>
        <class value="org.coode.annotate.AnnotateView"/>
        <headerColor value="@org.protege.classcolor"/>
        <category value="@org.protege.classcategory"/>
    </extension>

    <extension
         id="AnnotationViewPreferencesPanel"
         point="org.protege.editor.core.application.preferencespanel">
        <label value="Annotate preferences"/>
        <class value="org.coode.annotate.prefs.AnnotationViewPrefsPanel"/>
    </extension>

</plugin>

